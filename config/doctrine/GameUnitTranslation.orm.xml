<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="FrankProjects\UltimateWarfare\Entity\GameUnitTranslation" table="game_unit_translation">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" column="name" length="20">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nameMulti" column="name_multi" length="50">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="rowName" column="row_name" length="20">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="image" column="image" length="100">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="text" column="description" length="65535">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
